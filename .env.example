# TubeAPI Configuration
# Copy this file to .env and fill in your values

# Required: API password for authentication
API_PASSWORD=your-secret-password-here

# Optional: Rate limiting (default: 10/minute)
# Examples: "10/minute", "100/hour", "1000/day"
RATE_LIMIT=10/minute

# Optional: Server port (default: 8000)
PORT=8000

# Optional: Download directory (default: system temp)
OUTPUT_DIR=/tmp/yt_downloads
